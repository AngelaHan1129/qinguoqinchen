{
  "name": "qinguoqinchen-ai-2025",
  "version": "0.0.1",
  "description": "侵國侵城 AI 紅隊多模態滲透測試與防禦評估系統",
  "author": "侵國侵城團隊",
  "private": true,
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"{src,test}/**/*.js\" --fix",
    "clean": "rimraf dist coverage",
    "start": "node src/main.js",
    "start:dev": "nodemon",
    "start:debug": "nodemon --inspect",
    "start:prod": "NODE_ENV=production node src/main.js",
    "start:simple": "node simple-server.js",
    "start:simple-dev": "nodemon simple-server.js",
    "build": "echo 'JavaScript 專案無需編譯'",
    "prebuild": "rimraf dist",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=src",
    "docs:serve": "echo '🚀 侵國侵城 API 文檔運行在: http://localhost:7939/api/docs'",
    "health:check": "curl -f http://localhost:7939/health || echo '❌ 系統未運行'",
    "attack:demo": "curl -X POST http://localhost:7939/ai-attack/execute -H 'Content-Type: application/json' -d '{\"vectorIds\":[\"A1\",\"A3\"],\"intensity\":\"high\"}'",
    "attack:vectors": "curl -s http://localhost:7939/ai-attack/vectors | json_pp",
    "system:stats": "curl -s http://localhost:7939/system/stats | json_pp",
    "dev:full": "concurrently \"pnpm run docker:up\" \"pnpm run start:dev\"",
    "logs:tail": "tail -f logs/app.log"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.7.0",
    "@google-cloud/storage": "^7.17.2",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/serve-static": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "neo4j-driver": "^6.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "express-rate-limit": "^7.2.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^11.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.0",
    "supertest": "^7.0.0",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.7",
    "cross-env": "^7.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.js$",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/main.js"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "testTimeout": 30000,
    "setupFilesAfterEnv": [
      "<rootDir>/../test/setup.js"
    ],
    "verbose": true
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "src/**/*.spec.js",
      "node_modules/**/*",
      "logs/**/*",
      "coverage/**/*"
    ],
    "exec": "node src/main.js",
    "delay": 1000,
    "env": {
      "NODE_ENV": "development"
    },
    "verbose": true,
    "restartable": "rs",
    "colours": true
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.18.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/qinguoqinchen/ai-penetration-system"
  },
  "keywords": [
    "ai",
    "security",
    "penetration-testing",
    "ekyc",
    "nestjs",
    "vertex-ai",
    "gemini",
    "deepfake",
    "stylegan3",
    "stable-diffusion",
    "simswap",
    "red-team",
    "cybersecurity",
    "biometric-security",
    "multi-modal-ai",
    "javascript",
    "pnpm"
  ],
  "config": {
    "port": 7939,
    "api_prefix": "api",
    "swagger_path": "api/docs",
    "log_level": "info"
  },
  "pnpm": {
    "overrides": {
      "semver": "^7.5.4"
    }
  }
}