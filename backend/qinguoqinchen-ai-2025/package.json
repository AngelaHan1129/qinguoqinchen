{
  "name": "qinguoqinchen-ai-2025",
  "version": "0.0.1",
  "description": "侵國侵城 AI 紅隊多模態滲透測試與防禦評估系統",
  "author": "侵國侵城團隊",
  "private": true,
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "start": "node src/main.js",
    "start:dev": "nodemon src/main.js",
    "start:babel": "babel-node src/main.js",
    "start:babel-dev": "nodemon --exec babel-node src/main.js",
    "start:simple": "node simple-server.js",
    "start:simple-dev": "nodemon simple-server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.7.0",
    "@google-cloud/storage": "^7.17.2",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^4.19.2",
    "neo4j-driver": "^6.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.24.0",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@nestjs/testing": "^11.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "src",
    "testRegex": ".spec.js$",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
